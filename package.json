{
  "name": "ng-kloten",
  "version": "0.0.0",
  "scripts": {
    "startApi": "npx json-server --watch ./_demo-app-server/api-responses.json --routes ./_demo-app-server/api-routes.json --middlewares ./_demo-app-server/api-middlewares.js --port 1526",
    "startUI": "ng serve --proxy-config ./_demo-app-server/proxy.config.json --port 1848 --open",
    "start": "npm run startApi & npm run startUI",
    "ng": "ng",
    "prebuild": "cd ./projects/ng-kloten/ && npm install && npm version prepatch",
    "build": "ng build",
    "postbuild": "npx scss-bundle -c ./projects/ng-kloten/scss-bundle-config.json",
    "build:prod": "npm run build && cd dist/ng-kloten && npm pack && mv *.tgz ../../releases/ && cd ../../releases/ && ls -lah",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.1",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~8.1.1",
    "@angular/compiler": "~8.1.1",
    "@angular/core": "~8.1.1",
    "@angular/forms": "~8.1.1",
    "@angular/platform-browser": "~8.1.1",
    "@angular/platform-browser-dynamic": "~8.1.1",
    "@angular/router": "~8.1.1",
    "core-js": "2.6.9",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1",
    "@angular/material": "7.3.3",
    "@angular/flex-layout": "^7.0.0-beta.24"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.1",
    "@angular-devkit/build-ng-packagr": "~0.801.1",
    "@angular/cli": "~8.1.1",
    "@angular/compiler-cli": "^8.1.1",
    "@angular/language-service": "~8.1.1",
    "@types/jasmine": "3.3.13",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "12.6.2",
    "codelyzer": "^5.0.1",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "0.15.0",
    "karma": "4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "ng-packagr": "^5.1.0",
    "protractor": "~5.4.0",
    "tar": "4.4.10",
    "ts-node": "8.1.0",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.0",
    "tslint": "5.18.0",
    "typescript": "3.4.5"
  }
}
