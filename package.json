{
  "name": "ng-kloten",
  "version": "0.0.0",
  "scripts": {
    "startApi": "npx json-server --watch ./_demo-app-server/api-responses.json --routes ./_demo-app-server/api-routes.json --middlewares ./_demo-app-server/api-middlewares.js --port 1526",
    "startUI": "npx ng serve --proxy-config ./_demo-app-server/proxy.config.json --port 1848 --open",
    "start": "npm run startApi & npm run startUI",
    "ng": "npx ng",
    "prebuild": "cd ./projects/ng-kloten/ && npm install && npm version prepatch",
    "build": "npx ng build",
    "postbuild": "npx scss-bundle -c ./projects/ng-kloten/scss-bundle-config.json",
    "build:prod": "npm run build && cd dist/ng-kloten && npm pack && mv *.tgz ../../releases/ && cd ../../releases/ && ls -lah",
    "test": "npx ng test --watch=false --browsers=ChromeHeadless --codeCoverage=true",
    "lint": "npx ng lint",
    "e2e": "npx ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.14",
    "@angular/cdk": "8.2.3",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/flex-layout": "8.0.0-beta.27",
    "@angular/forms": "8.2.14",
    "@angular/material": "8.2.3",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "core-js": "2.6.9",
    "rxjs": "6.5.4",
    "tslib": "1.10.0",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.21",
    "@angular-devkit/build-ng-packagr": "0.803.21",
    "@angular/cli": "8.3.21",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@types/jasmine": "3.3.13",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "12.6.3",
    "codelyzer": "5.0.1",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "json-server": "0.15.0",
    "karma": "4.2.0",
    "karma-chrome-launcher": "3.0.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "ng-packagr": "5.7.1",
    "protractor": "5.4.0",
    "scss-bundle": "2.5.1",
    "tar": "4.4.10",
    "ts-node": "8.3.0",
    "tsickle": "0.35.0",
    "tslint": "5.18.0",
    "typescript": "3.5.3"
  }
}
